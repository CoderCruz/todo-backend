generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Color {
  red
  blue
  green
  orange
  yellow
  indigo
  violet
  pink
  brown
}

model Task {
  id        Int      @id @default(autoincrement())
  title     String
  color     Color
  completed Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
